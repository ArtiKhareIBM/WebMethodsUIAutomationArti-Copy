<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<modelVersion>4.0.0</modelVersion>

	<groupId>com</groupId>
	<artifactId>webMethodsFlowUI</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>jar</packaging>

	<name>webMethodsFlowUI</name>
	<url>http://maven.apache.org</url>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>

		<suiteXmlFile>./PredevSuitesjob.xml</suiteXmlFile>
	</properties>

	<build>
		<plugins>
			<!-- Compiler plug-in -->
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.4</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>

			<!-- Below plug-in is used to execute tests -->
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.18.1</version>
				<configuration>
				<argLine>-Xmx10000m</argLine>
					<systemPropertyVariables>
						<property>
							<name>userName</name>
							<value>${userName}</value>
						</property>

						<property>
							<name>password</name>
							<value>${password}</value>
						</property>

						<property>
							<name>url</name>
							<value>${url}</value>
						</property>
						
						<property>
							<name>explicitWait</name>
							<value>${explicitWait}</value>
						</property>
						
						<property>
							<name>ISpassword</name>
							<value>${ISpassword}</value>
						</property>

						<property>
							<name>ISuserName</name>
							<value>${ISuserName}</value>
						</property>

						<property>
							<name>ISUrl</name>
							<value>${ISUrl}</value>
						</property>

					</systemPropertyVariables>


					<suiteXmlFiles>
						<!-- TestNG suite XML files -->
						<suiteXmlFile>src/test/java/PredevSuitesjob.xml</suiteXmlFile>
					</suiteXmlFiles>

				</configuration>
			</plugin>
		</plugins>
	</build>



	<dependencies>

